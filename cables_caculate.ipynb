    "# -*- coding=utf-8 -*-\n",
    "\n",
    "import aiml\n",
    "\n",
    "# 创建Kernel()和 AIML 学习文件\n",
    "kernel = aiml.Kernel()\n",
    "kernel.learn(\"std-startup.xml\")\n",
    "kernel.respond(\"load aiml b\")\n",
    "\n",
    "# 按组合键 CTRL-C 停止循环\n",
    "while True:\n",
    "    print kernel.respond(raw_input(\"Enter your message >> \"))"
